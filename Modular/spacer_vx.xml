<!DOCTYPE template>
<!--
Description: Spacer with fixed sizes
-->
<Template>
  <WidgetGroup>
    <ObjectName>Spacer_<Variable name="width"/>x<Variable name="height"/></ObjectName>
    <Layout>horizontal</Layout>
    <Size><Variable name="width"/>,<Variable name="height"/></Size>
    <Style>
      /** /
      QGroupBox {
        border              : 1px solid green;
        background-color    : #003300;
      }
      /**/
    </Style>
  </WidgetGroup>
</Template>




